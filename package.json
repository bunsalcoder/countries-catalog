{
  "name": "countries-catalog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "eslint . --ext .js,.vue",
    "lint:fix": "eslint . --ext .js,.vue --fix"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "core-js": "^3.8.3",
